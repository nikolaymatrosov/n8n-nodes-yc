{
  "name": "@nikolaymatrosov/n8n-nodes-yc",
  "version": "0.5.4",
  "description": "n8n nodes for Yandex Cloud services",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Nikolay Matrosov",
    "email": "nikolay.matrosov@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolaymatrosov/n8n-nodes-yc.git"
  },
  "engines": {
    "node": ">=22.15.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && tsc-alias && gulp build:icons && npm run validate:nodes",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate:nodes": "node scripts/validate-nodes.js",
    "prepublishOnly": "npm run build && eslint -c eslint.config.prepublish.js --ignore-pattern '**/*.test.ts' nodes credentials package.json",
    "git-tag": "git tag v`cat package.json | jq -r '.version' | awk -F. '{print $1}'` -f &&  git tag v`cat package.json | jq -r '.version'` -f",
    "changelog:promote": "node scripts/promote-changelog.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/YandexCloudAuthorizedApi.credentials.js",
      "dist/credentials/YandexCloudFomoApi.credentials.js",
      "dist/credentials/YandexCloudGptApi.credentials.js",
      "dist/credentials/YandexCloudStaticApi.credentials.js",
      "dist/credentials/YandexCloudYdbApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/YandexCloudFomo/YandexCloudFomo.node.js",
      "dist/nodes/LMChatYandexGpt/LmChatYandexGpt.node.js",
      "dist/nodes/YandexCloudPostbox/YandexCloudPostbox.node.js",
      "dist/nodes/YandexCloudMessageQueue/YandexCloudMessageQueue.node.js",
      "dist/nodes/YandexCloudMessageQueue/YandexCloudMessageQueueTrigger.node.js",
      "dist/nodes/YandexCloudDataStreams/YandexCloudDataStreams.node.js",
      "dist/nodes/YandexCloudDataStreams/YandexCloudDataStreamsTrigger.node.js",
      "dist/nodes/YandexCloudFunctions/YandexCloudFunctions.node.js",
      "dist/nodes/YandexCloudObjectStorage/YandexCloudObjectStorage.node.js",
      "dist/nodes/YandexCloudSpeechKit/YandexCloudSpeechKit.node.js",
      "dist/nodes/YandexCloudSpeechKitSTT/YandexCloudSpeechKitStt.node.js",
      "dist/nodes/YandexCloudCompute/YandexCloudCompute.node.js",
      "dist/nodes/YandexCloudContainers/YandexCloudContainers.node.js",
      "dist/nodes/YandexCloudWorkflows/YandexCloudWorkflows.node.js",
      "dist/nodes/YandexCloudTranslate/YandexCloudTranslate.node.js",
      "dist/nodes/YandexCloudYDB/YandexCloudYdb.node.js",
      "dist/nodes/YandexCloudSearch/YandexCloudSearch.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.2",
    "@types/proxy-from-env": "^1.0.4",
    "@types/tmp": "^0.2.6",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "globals": "^16.5.0",
    "gulp": "^5.0.1",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "n8n-core": "^1.117.0",
    "nock": "^14.0.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-kinesis": "^3.922.0",
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/client-sesv2": "^3.922.0",
    "@aws-sdk/client-sqs": "^3.922.0",
    "@aws-sdk/core": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@aws-sdk/types": "^3.922.0",
    "@langchain/community": "^0.3.50",
    "@langchain/core": "^0.3.33",
    "@langchain/openai": "^0.6.16",
    "@n8n/json-schema-to-zod": "^1.5.0",
    "@yandex-cloud/nodejs-sdk": "3.0.0-beta.1",
    "@ydbjs/core": "^6.0.4",
    "@ydbjs/query": "^6.0.4",
    "@ydbjs/value": "^6.0.4",
    "generate-schema": "^2.6.0",
    "langchain": "0.3.33",
    "lodash": "^4.17.21",
    "nice-grpc": "^2.1.13",
    "openai": "^4.76.1",
    "proxy-from-env": "^1.1.0",
    "tmp-promise": "^3.0.3",
    "undici": "^7.16.0",
    "xml2js": "^0.6.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "resolutions": {
    "@langchain/core": "^0.3.33"
  },
  "overrides": {
    "@langchain/core": "^0.3.33"
  }
}
